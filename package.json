{
  "name": "aidbox",
  "version": "1.1.0",
  "description": "aidbox example plugin",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --forceExit && npm run lint",
    "lint": "eslint src/*.js",
    "docker-compose": "docker-compose -f docker-compose-test.yaml --project-name sdk up -d",
    "docker-compose-down": "docker-compose -f docker-compose-test.yaml --project-name sdk down",
    "docker-build": "docker build -t sdk_test -f Dockerfile-test .",
    "docker-run": "docker run --rm --network sdk_default --hostname app --name app  -p 8989:8989 --env-file .env-test -v $(pwd):/app -v $(pwd)/example/wait-for-it.sh:/usr/local/bin/wait-for-it.sh  sdk_test wait-for-it.sh devbox:8080 --strict --timeout=0 -- npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aidbox/aidbox-node-sdk.git"
  },
  "keywords": [
    "FHIR",
    "aidbox",
    "nodejs"
  ],
  "author": "Health Samurai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aidbox/aidbox-node-sdk/issues"
  },
  "homepage": "https://github.com/Aidbox/aidbox-node-sdk#readme",
  "dependencies": {
    "request": "^2.88.0",
    "ws": "^7.0.0"
  },
  "devDependencies": {
    "eslint": "^5.16.0",
    "jest": "^24.8.0"
  }
}
